{
  "manifest_version": 2,
  "name": "Zoom Downloader",
  "version": "1.0",
  "description": "Download Zoom recordings",
  "short_name": "zoom-downloader",
  "permissions": [
    "cookies",
    "downloads",
    "https://*/",
    "tabs"
  ],
  "browser_action": {
    "default_title": "Download Zoom Recording",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
